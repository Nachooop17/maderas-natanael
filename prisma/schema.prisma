// prisma/schema.prisma

generator client {
  provider = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db" 
}

model Producto {
  id          Int      @id @default(autoincrement())
  nombre      String
  precio      Float
  imagenUrl   String
  creadoEn    DateTime @default(now())
}